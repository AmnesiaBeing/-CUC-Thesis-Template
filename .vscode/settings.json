{
    "latex-workshop.latex.recipe.default":"lastUsed",
    "latex-workshop.latex.outDir":"out",
    "latex-workshop.latex.recipes":[
        {
            "name": "xelatex",
            "tools": [
                "xelatex"
            ]
        },
        {
            "name": "bibxe",
            "tools": [
              "xelatex",
              "biber",
              "xelatex",
              "xelatex"
            ]
          }
    ],
    "latex-workshop.latex.tools":[
        {
            "name": "xelatex", 
            "command": "xelatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-output-directory=%OUTDIR%",
                "%DOC%"
            ],
            "env": {}
        },
        {
            "name": "biber",
            "command": "biber",
            "args": [
              "%OUTDIR%/%DOCFILE%"
            ],
            "env": {}
          }
    ]
}